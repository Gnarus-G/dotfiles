{
  "nativeMCPServers": {
    "neovim": {
      "disabled": false
    }
  },
  "mcpServers": {
    "kokoro-tts-mcp": {
      "disabled": false,
      "args": [
        "--directory",
        "$HOME/.mcphub/servers/kokoro-tts-mcp",
        "run",
        "python",
        "mcp-tts.py"
      ],
      "env": {
        "HOME": "",
        "MP3_FOLDER": "/home/gnarus/Music/kokoro-tts",
        "TTS_SPEED": "0.8",
        "MP3_RETENTION_DAYS": "7",
        "TTS_LANGUAGE": "en-us",
        "S3_ENABLED": "false",
        "KOKORO_MODEL_PATH": "/home/gnarus/.mcphub/servers/kokoro-tts-mcp/kokoro-v1.0.onnx",
        "KOKORO_VOICES_PATH": "/home/gnarus/.mcphub/servers/kokoro-tts-mcp/voices-v1.0.bin",
        "DEBUG": "true",
        "TTS_VOICE": "af_nicole"
      },
      "command": "uv",
      "custom_instructions": {
        "text": ""
      }
    },
    "searxng": {
      "command": "npx",
      "disabled": false,
      "args": ["-y", "mcp-searxng"],
      "env": {
        "SEARXNG_URL": "https://metasearx.com/"
      }
    },
    "git": {
      "command": "uvx",
      "disabled": false,
      "args": ["mcp-server-git"]
    },
    "github.com/github/github-mcp-server": {
      "command": "docker",
      "disabled": false,
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "-e",
        "GITHUB_DYNAMIC_TOOLSETS=1",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/filesystem": {
      "command": "npx",
      "disabled": false,
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "$HOME"],
      "env": {
        "HOME": ""
      }
    },
    "web-browser-mcp-server": {
      "command": "uv",
      "args": ["tool", "run", "web-browser-mcp-server"],
      "env": {
        "REQUEST_TIMEOUT": "30"
      }
    },
    "github.com/zcaceres/fetch-mcp": {
      "command": "node",
      "disabled": false,
      "args": ["/home/gnarus/.mcphub/servers/fetch-mcp/dist/index.js"]
    }
  }
}
