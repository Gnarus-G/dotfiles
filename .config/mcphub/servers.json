{
  "nativeMCPServers": {
    "neovim": {
      "disabled": false
    }
  },
  "mcpServers": {
    "searxng": {
      "disabled": false,
      "args": ["-y", "mcp-searxng"],
      "env": {
        "SEARXNG_URL": "https://metasearx.com/"
      },
      "command": "npx"
    },
    "github.com/zcaceres/fetch-mcp": {
      "args": ["/home/gnarus/.mcphub/servers/fetch-mcp/dist/index.js"],
      "disabled": false,
      "command": "node"
    },
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/filesystem": {
      "args": [
        "run",
        "-i",
        "--rm",
        "--mount",
        "type=bind,src=/home/gnarus/,dst=/home",
        "mcp/filesystem",
        "/home"
      ],
      "disabled": false,
      "command": "docker"
    }
  }
}
